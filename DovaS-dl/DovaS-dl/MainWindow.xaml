<Window x:Class="DovaS_dl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DovaS_dl"
        mc:Ignorable="d"
        Title="DovaS-dl" Height="124" Width="280" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="StartIdBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="EndIdBox" HorizontalAlignment="Left" Height="23" Margin="135,10,0,0" TextWrapping="Wrap" Text="10" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="StartBtn" Content="Start" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="75" Click="StartBtn_Click"/>
        <Button x:Name="AbortBtn" Content="Abort" HorizontalAlignment="Left" Margin="90,38,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False" Click="AbortBtn_Click"/>
        <TextBlock x:Name="CurrentIdBox" HorizontalAlignment="Left" Margin="10,62,0,0" TextWrapping="Wrap" Text="Ready" VerticalAlignment="Top"/>

    </Grid>
</Window>
